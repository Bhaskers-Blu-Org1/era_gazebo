<launch>

	<node  name="detection_tensorflow" pkg="era_gazebo" type="detection_tensorflow.py" output="screen">
	
		<param name="use_sim_time" value="false" />
		<param name="use_gpu" value="false" />
		<param name="use_mask" value="true" />
		<param name="use_timers" value="true"/>
		<param name="model" type="string" value="$(find era_gazebo)/models/ssd_mobilenet_v1_coco_2018_01_28/frozen_inference_graph.pb" />
		
		<param name="model_zoo_path" type="string" value="$(optenv HOME)/local/ext/tensorflow/models"/>
		
		<remap from="image_input" to="/camera/color/image_raw" />

	</node>

</launch>
